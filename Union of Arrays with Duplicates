//by using set
class Solution {
  public:
    vector<int> findUnion(vector<int>& a, vector<int>& b) {
        set <int>s; //create a set s to store the value
        
        for(int i=0;i<a.size();i++){
            s.insert(a[i]);
        }
        
        for(int j=0;j<b.size();j++){
            s.insert(b[j]);
        }
        return vector<int>(s.begin(), s.end());  //because the function wnat in vector form

    }
};
